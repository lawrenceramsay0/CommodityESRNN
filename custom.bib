	langid = {english},
	file = {NDX Higher Volatility than SPX.pdf:C\:\\Users\\lawre\\Zotero\\storage\\VQ728KSX\\NDX Higher Volatility than SPX.pdf:application/pdf;Rosati et al. - 2019 - Social media and stock price reaction to data brea.pdf:C\:\\Users\\lawre\\Zotero\\storage\\J62PHH6J\\Rosati et al. - 2019 - Social media and stock price reaction to data brea.pdf:application/pdf},
