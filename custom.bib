@article{ramsay_assessing_2022,
	title = {Assessing Oil Market Overbought and Oversold with Neural Networks},
	abstract = {Background: The price reporting agency sponsoring this research – Argus Media (Argus) – maintains a predictive data science product called ‘Argus Possibility Curves’ which utilises a {GAMLSS} statistical model to forecast prices for crude oil and refined products. These forecasts are used by trading firms to hedge money on the commodity markets. Before the {APC} forecast is sent to clients, it is quality controlled against the overbought and oversold methodology to understand if successful trades can be made. 
Purpose: Argus are looking to find out if neural networks could be used to predict the {OBOS} level and if they could potentially be more accurate than existing processes.
Method: The {APC} data was run through a selection of temporal neural networks and compared against each other and compared to a naive model. The accuracy of their prediction was then assessed to see if they could make successful trades.
Result: The neural networks easily outperformed the naive model and the {LSTM} and {GRU} models were the overall top performers in {OBOS} prediction. The {RNN} outperformed the {LSTM} and {GRU} in one category of {OBOS} prediction.},
	author = {Ramsay, Lawrence},
	date = {2022-05-07},
}

